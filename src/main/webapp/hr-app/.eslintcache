/../requests/JobRequests\";\nimport CustomizedSnackbar from \"../Other/CustomizedSnackbar\";\nimport DeletedJobModal from \"./DeletedJobModal\";\n\nconst renderTooltip = (props) => (\n    <Tooltip id=\"button-tooltip\" {...props}>\n        {isUserHrManager() ?\n            \"See all job applications and job details\" :\n            \"Apply to job and see job details\"\n        }\n    </Tooltip>\n);\n\n\nclass JobCard extends Component {\n\n    state = {\n        isDeleted : false,\n        message : '',\n        isShowMessage : false,\n        clickedJob : '',\n        isShowDeletedJob : false\n    }\n\n    goToJobDetailPage = (e,job) => {\n        e.preventDefault();\n        this.props.history.push(`/viewJobDetails/${job.id}`);\n\n    }\n\n    goToEditJobPage = (e,jobId) => {\n        e.preventDefault();\n        this.props.history.push(`/editJob/${jobId}`);\n    }\n\n    showDeleteModal = (e,job) => {\n        e.preventDefault()\n        this.setState({\n            isShowDeletedJob : true,\n            clickedJob : job\n        })\n    }\n\n    deleteJob = async (e,jobId) => {\n\n        e.preventDefault();\n        const response = await sendDeleteJobRequest(jobId);\n\n        this.setState({\n\n            isDeleted : response.data.success,\n            message : response.data.message\n\n        }, () => {\n            const {isDeleted,message} = this.state;\n\n            this.showMessage();\n\n            if(isDeleted) {\n                setTimeout(() => {\n                    this.props.handleDelete(jobId);\n                },1500)\n            }\n\n        })\n    }\n\n    showMessage = () =>{\n        this.setState({\n            isShowMessage : true,\n        })\n    }\n\n    closeMessage = () => {\n        this.setState({\n            isShowMessage : false\n        })\n    }\n\n    closeDeletedJobModal = () => {\n\n        this.setState({\n            isShowDeletedJob : false\n        })\n    }\n\n    render() {\n        const {job} = this.props;\n        const {isDeleted,message,isShowMessage,isShowDeletedJob,clickedJob} = this.state;\n        return (\n            <div className={\"col-md-6 float-left mt-5\"}>\n                <Card>\n                    <Card.Header as=\"h5\">{job.title}</Card.Header>\n                    <Card.Body>\n                        <Card.Title> Son başvuru tarihi : {formatDate(job.lastApplicationDate)}</Card.Title>\n                        <div>\n                            <OverlayTrigger placement=\"right\"\n                                            delay={{ show: 250, hide: 400 }}\n                                            overlay={renderTooltip}>\n                                <Button variant=\"success\"\n                                        onClick={(e) => {this.goToJobDetailPage(e,job)}}>İlan Detayları\n                                </Button>\n                            </OverlayTrigger>\n                            {isUserHrManager() ?\n                                <div className={\"float-right\"}>\n\n                                    <Button variant={\"primary\"}\n                                            onClick={(e) => this.goToEditJobPage(e,job.id)}>Edit Job\n                                    </Button>\n\n                                    <Button variant={\"danger\"}\n                                            onClick={(e) => this.showDeleteModal(e,job)}>Delete Job\n                                    </Button>\n                                </div> :\n\n                                null\n                            }\n                        </div>\n                    </Card.Body>\n                </Card>\n                {isShowMessage ?\n                    <CustomizedSnackbar\n                        vertical={\"bottom\"}\n                        horizontal={\"right\"}\n                        open = {isShowMessage}\n                        handleClose = {this.closeMessage}\n                        message={message}\n                        messageType={isDeleted ? \"SUCCESS\" : \"ERROR\" }/>\n                    :\n                    null\n                }\n                {isShowDeletedJob ?  <DeletedJobModal job = {clickedJob}\n                                                      handleClose = {this.closeDeletedJobModal}\n                                                      handleDelete = {(e) => {this.deleteJob(e,clickedJob.id)}}/>\n                                                      :\n                    null\n                }\n            </div>\n        );\n    }\n}\n\nexport default withRouter(JobCard);","/home/emre/Desktop/Spring/Kod Gemisi/hr-app/src/main/webapp/hr-app/src/components/JobApplication/JobApplicationForm.js",["105"],"/home/emre/Desktop/Spring/Kod Gemisi/hr-app/src/main/webapp/hr-app/src/components/Homepage.js",[],"/home/emre/Desktop/Spring/Kod Gemisi/hr-app/src/main/webapp/hr-app/src/components/Login/LoginPage.js",[],"/home/emre/Desktop/Spring/Kod Gemisi/hr-app/src/main/webapp/hr-app/src/components/Other/CustomizedSnackbar.js",[],"/home/emre/Desktop/Spring/Kod Gemisi/hr-app/src/main/webapp/hr-app/src/components/JobApplication/JobApplications.js",[],"/home/emre/Desktop/Spring/Kod Gemisi/hr-app/src/main/webapp/hr-app/src/requests/JobApplicationRequests.js",["106"],"/home/emre/Desktop/Spring/Kod Gemisi/hr-app/src/main/webapp/hr-app/src/Helpers/DateFormat.js",[],"/home/emre/Desktop/Spring/Kod Gemisi/hr-app/src/main/webapp/hr-app/src/components/Applicant/ApplicantInformationModal.js",[],"/home/emre/Desktop/Spring/Kod Gemisi/hr-app/src/main/webapp/hr-app/src/components/Routes/HrManagerRoute.js",[],"/home/emre/Desktop/Spring/Kod Gemisi/hr-app/src/main/webapp/hr-app/src/components/Job/Forms/AddJobForm.js",[],"/home/emre/Desktop/Spring/Kod Gemisi/hr-app/src/main/webapp/hr-app/src/components/Job/Forms/EditJobForm.js",[],"/home/emre/Desktop/Spring/Kod Gemisi/hr-app/src/main/webapp/hr-app/src/components/Job/DeletedJobModal.js",[],{"ruleId":"107","replacedBy":"108"},{"ruleId":"109","r